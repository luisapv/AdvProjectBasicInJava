	<url-mapping id="enderecoCidade">
		<pattern value="/endereco/cidade/" />
		<view-id value="/pages/endereco/cidade/cidade_wrapper.jsf" />
		<action onPostback="false">#{cidadeBean.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="enderecoBairro">
		<pattern value="/endereco/bairro/" />
		<view-id value="/pages/endereco/bairro/bairro_wrapper.jsf" />
		<action onPostback="false">#{bairroBean.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="enderecoLogradouro">
		<pattern value="/endereco/logradouro/" />
		<view-id value="/pages/endereco/logradouro/logradouro_wrapper.jsf" />
		<action onPostback="false">#{logradouroBean.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="viewPDF">
		<pattern value="/showPDF/" />
		<view-id value="/pages/processo/vusalizarPDF.jsf" />
		<action>#{viewPDF.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="viewPDF-Paramter">
		<pattern value="/showPDF/#{ pdf : viewPDF.processoString }/" />
		<view-id value="/pages/processo/vusalizarPDF.jsf" />
		<action>#{viewPDF.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="viewPDF">
		<pattern value="/showMalaDireta/" />
		<view-id value="/pages/processo/vusalizarPDF.jsf" />
		<action>#{viewPDF.malaDiretaBean}</action>
	</url-mapping>

	<url-mapping id="AntigoBD">
		<pattern value="/importAntigoBD/" />
		<view-id value="/pages/importAntigoBD/importAntigoBD.jsf" />
	</url-mapping>


</pretty-config>