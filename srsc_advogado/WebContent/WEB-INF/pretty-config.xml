<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.3
   http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.3.xsd">

	<url-mapping id="login">
		<pattern value="/login/" />
		<view-id value="/pages/login/login.jsf" />
	</url-mapping>

	<url-mapping id="loginCadastro">
		<pattern value="/login/cadastro/" />
		<view-id value="/pages/login/login_wrapper.jsf" />
		<action onPostback="false">#{loginBean.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="advogado">
		<pattern value="/advogado/" />
		<view-id value="/pages/advogado/advogado_wrapper.jsf" />
		<action onPostback="false">#{advogadoBean.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="fase">
		<pattern value="/fase/" />
		<view-id value="/pages/fase/fase_wrapper.jsf" />
		<action onPostback="false">#{faseBean.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="materia">
		<pattern value="/materia/" />
		<view-id value="/pages/materia/materia_wrapper.jsf" />
		<action onPostback="false">#{materiaBean.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="pessoaFisica">
