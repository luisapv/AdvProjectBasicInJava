	</url-mapping>

	<url-mapping id="processo">
		<pattern value="/processo/" />
		<view-id value="/pages/processo/processo_wrapper.jsf" />
		<action onPostback="false">#{processoBean.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="endereco">
		<pattern value="/endereco/" />
		<view-id value="/pages/endereco/endereco_wrapper.jsf" />
		<action onPostback="false">#{enderecoBean.iniciarBean}</action>
	</url-mapping>

	<url-mapping id="enderecoCep">
		<pattern value="/cep/" />
		<view-id value="/pages/endereco/cep/cep_wrapper.jsf" />
		<action onPostback="false">#{cepBean.iniciarBean}</action>
